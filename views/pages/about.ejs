<link rel="stylesheet" href="/css/about.css">

<%- include('../partials/header') %>

<main>
    <section class = "about-header">
  <h1>Meet Our Event Organizers</h1>
  <p>Meet the dedicated organizers who bring our community’s events to life.</p>
  </section>
<ul class="pure-g team-list">
  <% organizers.forEach(org => { %>
    <li class="pure-u-1 pure-u-md-1-3 team-member">
      <img src="<%= org.image %>" alt="<%= org.name %>" class="organizer-img">
      <strong><%= org.name %></strong> – <strong><%= org.role %></strong>
      <p><%= org.description %></p>
    </li>
  <% }); %>
</ul>
</main>

<%- include('../partials/footer') %>
